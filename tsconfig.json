{
  "compilerOptions": {
    "target": "es6",
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "es6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "incremental": true
  },
  "exclude": ["node_modules"]
}
